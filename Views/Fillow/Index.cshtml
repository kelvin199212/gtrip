    <script>
            $(document).ready(function () {
                // Array of image URLs
                var images = [
                    'assets/images/banner/gtrippro_banner1.jpeg',
                    'assets/images/banner/gtrippro_banner2.jpeg',
                    'assets/images/banner/gtrippro_banner3.jpeg',
                    'assets/images/banner/gtrippro_banner4.jpg',
                    'assets/images/banner/gtrippro_banner5.jpg',
                    'assets/images/banner/gtrippro_banner6.jpg',
                    'assets/images/banner/gtrippro_banner7.jpg',
                    'assets/images/banner/gtrippro_banner8.jpg',
                    'assets/images/banner/gtrippro_banner9.jpeg',
                    // Add more image URLs as needed
                ];

            var $banner = $('.banner');
            var index = 0;

            function showNextImage() {
                index = (index + 1) % images.length;
                var newImageUrl = images[index];

                $banner.fadeOut(100, function () {
                    $banner.css('background-image', 'url(' + newImageUrl + ')');
                    $banner.animate({ width: 'toggle' }, 350);
                });
            }

            // Change background image every 5 seconds (5000 milliseconds)
            setInterval(showNextImage, 7000);

            // Initial image
            $banner.css('background-image', 'url(' + images[index] + ')');
            });
    </script>


